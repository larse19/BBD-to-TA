clock x;
int option;
int inserted_amount;
int product;
chan insert_2;
chan select_Coke;
chan select_ice;
chan cancel;
chan no_select;
chan pick_Coke;
chan no_pick_Coke;
</declaration>
	<template>
		<name>vending_machine</name>
		<location id="id0" x="0" y="0">
			<name x="8" y="8">idle</name>
		</location>
		<location id="id1" x="0" y="119">
			<name x="0" y="139">selection_mode</name>
		</location>
		<location id="id2" x="153" y="-102">
			<name x="170" y="-93">dispensing_mode</name>
		</location>
		<location id="id3" x="153" y="119">
			<name x="153" y="139">extra_dispensing_mode</name>
		</location>
		<location id="id4" x="0" y="-102">
			<name x="17" y="-93">returned</name>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="17" y="59">insert_2?</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="110" y="0">select_Coke?</label>
