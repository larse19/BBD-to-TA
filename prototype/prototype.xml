		</location>
		<location id="id6" x="200" y="0">
			<name x="200" y="20">selection_mode</name>
		</location>
		<location id="id7" x="300" y="0">
			<name x="300" y="20">dispensing_mode</name>
		</location>
		<location id="id8" x="400" y="0">
			<name x="400" y="20">extra_dispensing_mode</name>
		</location>
		<init ref="id5"/>
	</template>
	<system>
system vending_machine, user;
</system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
